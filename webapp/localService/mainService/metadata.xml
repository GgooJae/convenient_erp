            <EntityType Name="zcap_items" sap:content-version="1">
                <Key>
                    <PropertyRef Name="ItemId"/>
                </Key>
                <Property Name="ItemId" Type="Edm.String" Nullable="false" MaxLength="10" sap:label="ITEM_ID"/>

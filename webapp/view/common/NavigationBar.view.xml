<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    controllerName="convenienterp.controller.common.NavigationBar">
    <Bar>
        <contentLeft>
            <core:Icon src="sap-icon://home" size="25px" press="onHomeIconPress"/>
        </contentLeft>
        <contentRight>
            <core:Icon src="sap-icon://search" size="25px" class="myIconMargin" press="onSearchIconPress"/>
            <core:Icon src="sap-icon://bell" size="25px" class="myIconMargin" press="onBellIconPress"/>
            <core:Icon src="sap-icon://account" size="25px" class="myIconMargin" press="onAccountIconPress"/>
        </contentRight>
    </Bar>
</mvc:View>
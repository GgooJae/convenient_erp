{
  "_version": "1.12.0",
  "sap.app": {
    "id": "convenienterp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.1",
      "toolsId": "bac88432-d786-4056-9106-43f3d414b2bf"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZCAP_GW_SRV_02/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "convenienterp.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "convenienterp.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "pattern": "",
          "name": "RouteLogin",
          "target": "TargetLogin"
        },
        {
          "pattern": "",
          "name": "RouteLogin_2",
          "target": "TargetLogin_2"
        },
        {
          "name": "RouteLaunchpad",
          "pattern": "launchpad",
          "target": [
            "TargetLaunchpad"
          ]
        },
        {
          "name": "RouteStock_Order",
          "pattern": "stockorder",
          "target": [
            "TargetStock_Order"
          ]
        },
        {
          "name": "RouteStock_Order_History",
          "pattern": "stockorderhistory",
          "target": [
            "TargetStock_Order_History"
          ]
        },
        {
          "name": "RouteStock_Order_History_HO",
          "pattern": "stockorderhistoryho",
          "target": [
            "TargetStock_Order_History_HO"
          ]
        },
        {
          "name": "RouteStock_Order_Schedule",
          "pattern": "stockorderschedule",
          "target": [
            "TargetStock_Order_Schedule"
          ]
        },
        {
          "name": "RouteCheck_Inventory",
          "pattern": "checkinventory",
          "target": [
            "TargetCheck_Inventory"
          ]
        }
      ],
      "targets": {
        "TargetLaunchpad": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Launchpad",
          "viewName": "Launchpad"
        },
        "TargetStock_Order": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Stock_Order",
          "viewName": "Stock_Order"
        },
        "TargetLogin": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Login",
          "viewName": "Login"
        },
        "TargetLogin_2": {
          "viewType": "XML",
          "transition": "show",
          "clearControlAggregation": false,
          "viewId": "Login",
          "viewName": "Login"
        },
        "TargetStock_Order_History": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Stock_Order_History",
          "viewName": "Stock_Order_History"
        },
        "TargetStock_Order_History_HO": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Stock_Order_History_HO",
          "viewName": "Stock_Order_History_HO"
        },
        "TargetStock_Order_Schedule": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Stock_Order_Schedule",
          "viewName": "Stock_Order_Schedule"
        },
        "TargetCheck_Inventory": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Check_Inventory",
          "viewName": "Check_Inventory"
        }
      }
    },
    "rootView": {
      "viewName": "convenienterp.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "components": {
      "convenienterp.nav": {
        "name": "convenienterp.nav",
        "settings": {
          "id": "navComponent"
        }
      }
    }
  }
}